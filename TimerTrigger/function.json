{
  "scriptFile": "main.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */1 * * * *"
    },
    {
      "name": "blobout",
      "type": "blob",
      "direction": "out",
      "path": "output/characters/{datetime:yyyy}/{datetime:MM}/{datetime:dd}/{datetime:yyyyMMddss}extract.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
