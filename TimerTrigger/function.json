{
  "scriptFile": "main.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 40 * * * *"
    },
    {
      "name": "blobout",
      "type": "blob",
      "direction": "out",
      "path": "output/characters/{datetime:yyyy}/{datetime:MM}/{datetime:dd}/{datetime:HH}/characters-{datetime:yyyyMMddHHmm}.json",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "msg",
      "type": "queue",
      "direction": "out",
      "queueName": "outqueue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
